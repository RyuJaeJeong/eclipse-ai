<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.finance.eclipse.suggestion.AiStartup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.finance.eclipse.suggestion.commands.inlineCompletion.accept"
            contextId="com.finance.eclipse.suggestion.inlineCompletionVisible"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="TAB">
      </key>
      <key
            commandId="com.finance.eclipse.suggestion.commands.inlineCompletion.dismiss"
            contextId="com.finance.eclipse.suggestion.inlineCompletionVisible"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ESC">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.finance.eclipse.suggestion.commands.inlineCompletion"
            name="AI Code Completion">
      </category>
      <command
            categoryId="com.finance.eclipse.suggestion.commands.inlineCompletion"
            id="com.finance.eclipse.suggestion.commands.inlineCompletion.accept"
            name="Accept Inline Completion">
      </command>
      <command
            categoryId="com.finance.eclipse.suggestion.commands.inlineCompletion"
            id="com.finance.eclipse.suggestion.commands.inlineCompletion.dismiss"
            name="Dismiss Inline Completion">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.finance.eclipse.suggestion.handlers.AcceptHandler"
            commandId="com.finance.eclipse.suggestion.commands.inlineCompletion.accept">
      </handler>
      <handler
            class="com.finance.eclipse.suggestion.handlers.DismissHandler"
            commandId="com.finance.eclipse.suggestion.commands.inlineCompletion.dismiss">
      </handler>
   </extension>

</plugin>
